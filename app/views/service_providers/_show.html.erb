<div id="mySPshow">
  <div class="card">
    <div class="center-text">
      <%= image_tag @sp.image.url(:medium), class:'w200 circle' %>
    </div>
    <div class="center-text">
      <%= @sp.fl_name %>
    </div>
    <div class="center-text">
      <i class="fa fa-phone"></i> <%= @sp.phone %>
    </div>
    <div class="center-text">
      Gender: <%= @sp.gender %>
    </div>
    <div class="center-text">
      Region: <%= @sp.region.name %>
    </div>
  </div>
</div>
