<%= form_for @user do |u| %>
  <div class="form-group">
    <%= u.text_field :username, placeholder: 'username' %>
  </div>
  <div class="form-group">
    <%= u.text_field :email, placeholder: 'email' %>
  </div>
  <div class="form-group">
    <%= u.password_field :password, placeholder: 'password', id:'psw1' %>
  </div>
  <div class="form-group">
    <%= u.password_field :password_confirmation, placeholder: 'confirm password', id:'pwd2' %>
  </div>
  <div class="form-group">
    <%= u.submit 'SUnbmit' %>
  </div>
<% end %>

